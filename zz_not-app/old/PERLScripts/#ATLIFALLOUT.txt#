var tabexp=new Array();var imagexp=new Array();
var itabexp=0;
var winhelp=null;
function resethelp()
{
if(winhelp!=null){
  if(!winhelp.closed)winhelp.close();
  winhelp=null;
  }
}
function gethelp(docum,w,h)
{
s='/dendien/scripts/tlfiv5/showp.exe?13;s=3509479065;p=y target=x';
k=s.indexOf('p=')+2;
s=s.substring(0,k);
winhelp=window.open(s+docum,'Aide','toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=1,width='+w+',height='+h+',top=5,left=20');
winhelp.focus();}
function gethelp2(docum,w,h)
{
winhelp=window.open(docum,'Aide','toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=1,width='+w+',height='+h+',top=5,left=20');
winhelp.focus();}
</script>
